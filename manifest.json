{
  "manifest_version": 2,
  "name": "Lycoris",
  "version": "0.3.9",
  "description": "Enhance Monorail.",
  "permissions": [
    "https://bugs.chromium.org/*",
    "*://chromium.googlecode.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://bugs.chromium.org/*p/*/issues*"],
      "js": ["lycoris_preview.js"],
      "css": ["lycoris_preview.css"]
    }
  ]
}